{
  "singleQuote": false,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 80,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "importOrderSortSpecifiers": true,
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "interface",
    "(?=content|api)",
    "(components/|./index)",
    "^components/(.*)$",
    "^[./]",
    "mock/",
    "config",
    "utils/",
    "hooks/",
    "(?=./styles.module.scss)",
    ".+\\.(png|jpg|jpeg|gif|svg)$",
    "^../(.*)$"
  ]
}
